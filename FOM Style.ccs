      <Name>Gesetzeskommentar = Loseblattsammlung</Name>
      <Notes />
      <Macro><![CDATA[using System;
using System.Linq;
using System.Collections.Generic;
using SwissAcademic.Citavi;
using SwissAcademic.Citavi.Metadata;
using SwissAcademic.Collections;

namespace SwissAcademic.Citavi.Citations
{
	public class CustomTemplateCondition
		:
		ITemplateConditionMacro
	{
		//Es wird (mindestens ein) Beitrag aus diesem Gesetzeskommentar zitiert, der im Feld "Ergänzungslieferung/Volume" Daten enthält.
		public bool IsTemplateForReference(ConditionalTemplate template, Citation citation)
		{
			if (citation == null) return false;
			if (citation.Reference == null) return false;
			if (citation.Reference.ReferenceType != ReferenceType.LegalCommentary) return false;
			
			var legalCommentary = citation.Reference;
			
			CitationManager citationManager = citation.CitationManager;
			if (citationManager == null) return false;
			if (citationManager.PlaceholderCitations == null || !citationManager.PlaceholderCitations.Any()) return false;
			
			
			Func<PlaceholderCitation, Reference> getContribution = (placeholderCitation) => {
				if (placeholderCitation == null) return null;
				if (placeholderCitation.Reference == null) return null;
				if (placeholderCitation.Reference.ReferenceType != ReferenceType.ContributionInLegalCommentary) return null;
				if (placeholderCitation.Reference.ParentReference == null) return null;
				if (placeholderCitation.Reference.ParentReference.Id.Equals(legalCommentary.Id)) return placeholderCitation.Reference;
				return null;
			};
			
			IEnumerable<Reference> distinctQuotedContributions = DistinctBy(citationManager.PlaceholderCitations, getContribution);
			if (!distinctQuotedContributions.Any()) return false;
			if (distinctQuotedContributions.Any(contribution => !string.IsNullOrWhiteSpace(contribution.Volume)))
			{
				return true;
			}
			
			return false;
		}
		
		public IEnumerable<Reference> DistinctBy(IEnumerable<PlaceholderCitation> source, Func<PlaceholderCitation, Reference> keySelector)
		{
		    HashSet<Reference> knownKeys = new HashSet<Reference>();
		    foreach (PlaceholderCitation placeholderCitation in source)
		    {
				Reference reference = keySelector(placeholderCitation);
		        if (reference != null && knownKeys.Add(reference))
		        {
		            yield return reference;
		        }
		    }
		}
	}
}]]></Macro>
    </CustomTemplateCondition>
